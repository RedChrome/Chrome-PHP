{
    "info": "File for MySQL, saves alle queries sent by core",

    "authenticationDoesIdAndTokenExist": "SELECT `id` FROM cpp_? WHERE `id` = ? AND `cookie_token` = \"?\" LIMIT 0,1",
    "authenticationUpdateTokenById": "UPDATE ? SET `cookie_token` = ? WHERE `id` = ? LIMIT 0,1",
    "authenticationGetPasswordAndSaltByIdentity": "SELECT `password`,`password_salt` FROM cpp_authenticate WHERE `id` = \"?\" LIMIT 0,1",
    "authenticationUpdateTimeById": "UPDATE cpp_authenticate SET `time` = ? WHERE `id` = ? LIMIT 0,1",
    "authenticationGetOdByPassword": "SELECT `id` FROM cpp_authenticate WHERE `password` = \"?\" AND `password_salt` = \"?\" ORDER BY `id` DESC LIMIT 0,1",

    "authorisationGetAccessById": "SELECT `_access` FROM cpp_authorisation_resource_default WHERE `_resource_id` = \"?\" AND `_transformation` = \"?\"",
    "authorisationGetUserGroupById": "SELECT `group_id` FROM cpp_authorisation_user_default WHERE `user_id`= \"?\" ",

    "configLoadConfiguration": "SELECT `name`,`subclass`, `value` FROM cpp_config",
    "configSetConfiguration": "INSERT INTO cpp_config VALUES(?, ?, ?, ?, ?)"

}