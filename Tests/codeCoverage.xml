<phpunit stopOnFailure="false">
    <testsuite name="Chrome-PHP">
    </testsuite>

<filter>
    <blacklist>
        <directory suffix=".php">./</directory>
    </blacklist>
</filter>

<logging>
    <!-- Put in target your own dir-->
    <log type="coverage-html" target="../../CHROME_2 Tools/CodeCoverage/" charset="UTF-8" yui="true" highlight="true"/>
</logging>

</phpunit>